<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- Beans definitions below -->
    <bean id="name" class="java.lang.String">
<!--        <constructor-arg value="John Doe"></constructor-arg>-->
        <constructor-arg value="John Doe" />
    </bean>

    <bean id="age" class="java.lang.Integer">
<!--        <constructor-arg value="John Doe"></constructor-arg>-->
        <constructor-arg value="24" />
    </bean>

    <context:component-scan base-package="com.dgmf.examples.h1"></context:component-scan>

<!-- 40.2.8 the context schema : https://docs.spring.io/spring-framework/docs/4.2.x/spring-framework-reference/html/xsd-configuration.html -->
</beans>